{
  "manifest_version": 1,
  "name": "FreshRSS-Notify",
  "version": "1.0",

  "options_ui": {
    "page": "options/index.html"
  },

  "permissions": [
    "storage"
  ],

  "background": {
    "scripts": [
      "background/CONSTANT.js",
      "background/lib/adapter.js",
      "background/lib/EventManager.js",
      "background/lib/fetch.js",
      "background/lib/main.js"
    ]
  }
}