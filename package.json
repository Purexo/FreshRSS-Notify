{
  "name": "freshrss-notify",
  "version": "3.0.5",
  "description": "Alert new unreads feeds",
  "private": true,
  "scripts": {
    "start": "npm run start:firefox",
    "prestart:firefox": "node scripts/patch-manifest.js src/manifests/dev-firefox-manifest.patch.json",
    "start:firefox": "web-ext run --firefox-profile FreshRSS --keep-profile-changes --start-url about:debugging --verbose",
    "prestart:chrome": "node scripts/patch-manifest.js src/manifests/chrome-manifest.patch.json",
    "start:chrome": "start chrome --profile-directory=FreshRSS --load-extension=%CD% chrome://extensions/",
    "deploy": "npm run deploy:firefox",
    "deploy-dev": "npm run deploy:firefox:dev",
    "predeploy:firefox": "node scripts/patch-manifest.js src/manifests/firefox-manifest.patch.json",
    "deploy:firefox": "web-ext sign --id freshrss-notify@purexo.mom",
    "predeploy:firefox:dev": "node scripts/patch-manifest.js src/manifests/dev-firefox-manifest.patch.json",
    "deploy:firefox:dev": "web-ext sign --id freshrss-notify-dev@purexo.mom",
    "build": "npm run build:firefox:prod",
    "prebuild:firefox:prod": "node scripts/patch-manifest.js src/manifests/firefox-manifest.patch.json",
    "build:firefox:prod": "web-ext build",
    "webpack:firefox": "webpack --env.patch=src/manifests/dev-firefox-manifest.patch.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Purexo/FreshRSS-Notify.git"
  },
  "author": "Purexo (https://purexo.mom/)",
  "contributors": [
    "ZatsuneNoMokou (https://zatsunenomokou.eu/)"
  ],
  "license": "GNU GPL",
  "bugs": {
    "url": "https://github.com/Purexo/FreshRSS-Notify/issues"
  },
  "homepage": "https://github.com/Purexo/FreshRSS-Notify#readme",
  "devDependencies": {
    "@types/jquery": "^3.3.30",
    "@types/webpack": "^4.39.3",
    "css-loader": "^3.2.0",
    "html-loader": "^0.5.5",
    "jiff": "^0.7.3",
    "style-loader": "^1.0.0",
    "web-ext": "^3.2.0",
    "web-ext-types": "^3.2.1",
    "webextension-polyfill": "^0.5.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "bootstrap": "^4.3.1",
    "dompurify": "^2.0.6",
    "file-loader": "^4.2.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.4.1",
    "popper.js": "^1.16.0"
  },
  "dependencies": {},
  "webExt": {
    "ignoreFiles": [
      "**/*.map"
    ]
  }
}
